{
  "name": "peakroute",
  "version": "0.5.0",
  "description": "Replace port numbers with stable, named .localhost URLs. For humans and agents. (Formerly portless)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "peakroute": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "prepublishOnly": "bun run build",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "echo 'No e2e tests defined yet'",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "local",
    "development",
    "proxy",
    "localhost"
  ],
  "author": "Vercel Labs",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/faladev/peakroute.git"
  },
  "homepage": "https://github.com/faladev/peakroute#readme",
  "bugs": {
    "url": "https://github.com/faladev/peakroute/issues"
  },
  "dependencies": {
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  }
}
